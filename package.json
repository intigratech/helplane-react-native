{
  "name": "@helplane/react-native",
  "version": "1.0.0",
  "description": "HelpLane chat widget SDK for React Native",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepare": "bob build",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "react-native",
    "helplane",
    "chat",
    "widget",
    "support",
    "customer-support"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intigratech/helplane-react-native.git"
  },
  "author": "HelpLane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intigratech/helplane-react-native/issues"
  },
  "homepage": "https://github.com/intigratech/helplane-react-native#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.70.0",
    "react-native-webview": ">=11.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-builder-bob": "^0.23.0",
    "react-native-webview": "^13.6.0",
    "typescript": "^5.3.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
